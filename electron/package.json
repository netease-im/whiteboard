    "autoprefixer": "^10.2.5",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "electron": "^13.1.4",
    "electron-packager": "^15.2.0",
    "electron-reloader": "^1.2.1",
    "html-webpack-plugin": "^5.3.1",
    "postcss-loader": "^5.2.0",
